{
  "name": "mup-docker-deploy",
  "version": "0.2.0",
  "description": "Plugin for Meteor Up to deploy an app using a Dockerfile",
  "scripts": {
    "prepare": "npm run build",
    "build": "babel src --out-dir lib --sourceRoot=./ --copy-files --sourcemaps",
    "build:watch": "npm run build -s -- -w",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "zodern",
  "license": "MIT",
  "keywords": [
    "meteor-up"
  ],
  "main": "index.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2"
  },
  "dependencies": {
    "contains-path": "^1.0.0",
    "joi": "^12.0.0",
    "mkdirp": "^0.5.1",
    "random-seed": "^0.3.0",
    "rimraf": "^2.6.3",
    "tar": "^4.4.10",
    "uuid": "^3.2.1"
  }
}
